{
  "manifest_version": 3,
  "name": "aIrrange",
  "version": "1.0",
  "description": "AI-powered tool for organizing and managing your search results.",
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://www.google.com/search*"],
      "js": ["content.js"]
    }
  ],
  "permissions": ["storage"],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "128": "icons/icon.svg"
  }
}
